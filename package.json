{
  "name": "agent-explore",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "files": [
    "build",
    "bin"
  ],
  "bin": {
    "agent-explore": "./bin/bin.js"
  },
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test --watchAll=false",
    "release": "semantic-release",
    "agent-explore": "./bin/bin.js",
    "upgrade:next": "yarn add @veramo/core@next @veramo/credential-w3c@next @veramo/data-store@next @veramo/did-comm@next @veramo/did-jwt@next @veramo/did-resolver@next @veramo/message-handler@next @veramo/remote-client@next @veramo/selective-disclosure@next"
  },
  "homepage": "https://explore.veramo.dev",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "semi": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "commander": "^7.0.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "ci": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): :rocket: New version ${nextRelease.version} [skip ci] \n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@craco/craco": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/chart.js": "^2.9.30",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.15",
    "@types/md5": "^2.2.1",
    "@types/random-words": "^1.1.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@veramo-community/veramo-react": "^0.3.3",
    "@veramo/core": "^1.1.3-next.46",
    "@veramo/credential-w3c": "^1.1.3-next.46",
    "@veramo/data-store": "^1.1.3-next.46",
    "@veramo/did-comm": "^1.1.3-next.46",
    "@veramo/did-jwt": "^1.1.3-next.46",
    "@veramo/did-resolver": "^1.1.3-next.46",
    "@veramo/message-handler": "^1.1.3-next.46",
    "@veramo/remote-client": "^1.1.3-next.46",
    "@veramo/selective-disclosure": "^1.1.3-next.46",
    "antd": "^4.11.2",
    "chart.js": "^2.9.4",
    "craco-less": "^1.17.1",
    "date-fns": "^2.16.1",
    "did-resolver": "^3.1.0",
    "ethr-did-resolver": "^4.3.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "husky": "^4.3.8",
    "md5": "^2.3.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "random-words": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.6.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "semantic-release": "^17.4.3",
    "typescript": "^4.0.3",
    "web-did-resolver": "^2.0.4",
    "web-vitals": "^0.2.4"
  }
}
